{
  "name": "jays-blog",
  "private": true,
  "description": "A starter for a blog powered by Gatsby and Markdown",
  "version": "1.0.0",
  "author": "Jay Hankins <jay@jayhankins.me>",
  "bugs": {
    "url": "https://github.com/jay-hankins/jay-hankins.github.io/issues"
  },
  "dependencies": {
    "gatsby": "^2.0.76",
    "gatsby-image": "^2.0.22",
    "gatsby-plugin-feed": "^2.0.8",
    "gatsby-plugin-google-analytics": "^2.0.5",
    "gatsby-plugin-manifest": "^2.0.5",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.6",
    "gatsby-plugin-sri": "^1.0.5",
    "gatsby-plugin-typography": "^2.2.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-embed-gist": "^1.1.5",
    "gatsby-remark-images": "^2.0.4",
    "gatsby-remark-prismjs": "^3.2.0",
    "gatsby-remark-responsive-iframe": "^2.0.5",
    "gatsby-remark-smartypants": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.2",
    "gatsby-transformer-remark": "^2.1.6",
    "gatsby-transformer-sharp": "^2.1.3",
    "hash-sum": "^1.0.2",
    "prismjs": "^1.15.0",
    "react": "^16.5.1",
    "react-barcode": "^1.3.4",
    "react-css-transition-replace": "^3.0.3",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-transition-group": "^2.5.3",
    "react-typography": "^0.16.13",
    "typeface-ibm-plex-sans": "^0.0.61",
    "typeface-ibm-plex-serif": "^0.0.61",
    "typeface-inter": "^3.2.0",
    "typeface-sarabun": "^0.0.71",
    "typography": "^0.16.17",
    "typography-theme-wordpress-2016": "^0.15.10"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^2.0.1",
    "prettier": "^1.14.2"
  },
  "homepage": "https://jayhankins.me",
  "keywords": [
    "hankins"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jay-hankins/jay-hankins.github.io"
  },
  "scripts": {
    "dev": "gatsby develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "format": "prettier --write 'src/**/*.js' 'src/**/*.md'",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "build": "gatsby build",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js",
    "deploy": "rm -rf public/ .cache/ && gatsby build && gh-pages -d public -b master"
  }
}
